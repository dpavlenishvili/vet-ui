{
    "name": "ui",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "frontend/libs/ui/src",
    "prefix": "v-ui",
    "tags": [],
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/angular:package",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "frontend/libs/ui/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "frontend/libs/ui/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "frontend/libs/ui/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "frontend/libs/ui/jest.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"]
        }
    }
}
