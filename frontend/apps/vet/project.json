{
    "name": "vet",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "prefix": "app",
    "sourceRoot": "frontend/apps/vet/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@angular-devkit/build-angular:browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/frontend/apps/vet/browser",
                "index": "frontend/apps/vet/src/index.html",
                "main": "frontend/apps/vet/src/main.ts",
                "polyfills": ["zone.js"],
                "tsConfig": "frontend/apps/vet/tsconfig.app.json",
                "assets": ["frontend/apps/vet/src/favicon.ico", "frontend/apps/vet/src/assets"],
                "styles": ["frontend/apps/vet/src/styles.scss", "frontend/apps/vet/src/assets/icons.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "extractLicenses": false,
                    "sourceMap": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@angular-devkit/build-angular:dev-server",
            "options": {
                "port": 4201
            },
            "configurations": {
                "production": {
                    "buildTarget": "vet:build:production"
                },
                "development": {
                    "buildTarget": "vet:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "buildTarget": "vet:build"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "frontend/apps/vet/jest.config.ts"
            }
        },
        "server": {
            "executor": "@angular-devkit/build-angular:server",
            "options": {
                "outputPath": "dist/frontend/apps/vet/server",
                "main": "frontend/apps/vet/server.ts",
                "tsConfig": "frontend/apps/vet/tsconfig.server.json"
            },
            "configurations": {
                "production": {
                    "outputHashing": "media"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "sourceMap": true,
                    "extractLicenses": false
                }
            },
            "defaultConfiguration": "production"
        },
        "serve-ssr": {
            "executor": "@angular-devkit/build-angular:ssr-dev-server",
            "configurations": {
                "development": {
                    "browserTarget": "vet:build:development",
                    "serverTarget": "vet:server:development"
                },
                "production": {
                    "browserTarget": "vet:build:production",
                    "serverTarget": "vet:server:production"
                }
            },
            "defaultConfiguration": "development"
        },
        "prerender": {
            "executor": "@angular-devkit/build-angular:prerender",
            "options": {
                "routes": ["/"]
            },
            "configurations": {
                "production": {
                    "browserTarget": "vet:build:production",
                    "serverTarget": "vet:server:production"
                },
                "development": {
                    "browserTarget": "vet:build:development",
                    "serverTarget": "vet:server:development"
                }
            },
            "defaultConfiguration": "production"
        }
    }
}
